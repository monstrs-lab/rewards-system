{
  "name": "@rewards-system/reward-agents-rpc-client",
  "version": "0.0.3",
  "license": "MIT",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": "./src/index.ts"
  },
  "main": "src/index.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn library build",
    "prepack": "yarn run build",
    "postpack": "rm -rf dist"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^1.3.1",
    "@connectrpc/connect": "^0.13.2",
    "@connectrpc/connect-node": "^0.13.2",
    "@rewards-system/rewards-rpc": "workspace:0.0.3"
  },
  "publishConfig": {
    "exports": {
      "./package.json": "./package.json",
      ".": {
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "main": "dist/index.js",
    "typings": "dist/index.d.ts"
  }
